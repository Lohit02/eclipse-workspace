package datastructures;

import java.util.Scanner;

public class bekar
{

	public static void main(String[] args) {
		 Scanner sc= new Scanner(System.in);
			int n = sc.nextInt();
			int res= factorial(n);
			System.out.println(res);
		    
		}
	public static int factorial(int n)
	{
		if(n<=2)
		{
			return n;
		}return n*factorial(n-1);
	}
}


