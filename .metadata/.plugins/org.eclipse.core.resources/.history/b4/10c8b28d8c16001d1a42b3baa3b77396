package w3resource;

public class magic_num1
{

	public static void main(String[] args) 
	{
		magic_num ob=new magic_num();
		int n=325;
		//int r=sum;
		System.out.println(ob.Sum(n));
	}
		int Sum(int n) 
		{
			int sum=0;
			int n1;
			while(n!=0) 
			{
				n1=n%10;
				sum=sum+n1;
				n=n/10;
			}
			return sum;
		}
}
			
		

	


