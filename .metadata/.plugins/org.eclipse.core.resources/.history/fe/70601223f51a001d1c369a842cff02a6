package datastructures;

import java.util.Scanner;

public class bekar {

	public static void main(String[] args) {
		 Scanner sc= new Scanner(System.in);
			int t = sc.nextInt();
			for(int i=0;i<t;i++)
			{
			    int p = sc.nextInt();
			    int q = sc.nextInt();
			   if(p>=1 && q>=1)
			   {
			       if(p==q){
			           System.out.println("YES");
			       }else
			       System.out.println("NO");
			   }
			   System.out.println("NO");
			}
		    
		}

	}

}
