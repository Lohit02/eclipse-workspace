package array;

import java.util.Scanner;
  //sum the elements of the array
public class Array {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the size of array:");
		int n=sc.nextInt();
		int array[]= new int[n];
		for(int i=0;i<n;i++)
		{
			array[i]= sc.nextInt();
		}
		int sum=0;
		for(int i=0;i<n;i++)
		{
			sum= sum+ array[i];
			System.out.println(sum);
		}
		
		

	}

}
